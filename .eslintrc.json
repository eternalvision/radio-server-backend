{
  "env": {
    "node": true,
    "es2022": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:import/recommended", "prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error",
    "import/no-unresolved": "off"
  }
}
